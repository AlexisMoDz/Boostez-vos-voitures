--[[----------BOOSTE VOITURE POLICE----------]]
Citizen.CreateThread(function()
   while true do
    Citizen.Wait(0)
    if IsVehicleModel(GetVehiclePedIsIn(GetPlayerPed(-1), true), GetHashKey("police")) then
       SetVehicleEnginePowerMultiplier(GetVehiclePedIsIn(GetPlayerPed(-1), true), 2.9)
       SetVehicleEngineTorqueMultiplier(GetVehiclePedIsIn(GetPlayerPed(-1), true), 2.9)
     end
   end
 end)

 Citizen.CreateThread(function()
   while true do
    Citizen.Wait(0)
    if IsVehicleModel(GetVehiclePedIsIn(GetPlayerPed(-1), true), GetHashKey("police1")) then
